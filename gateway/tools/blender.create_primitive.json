{
  "id": "blender.create_primitive",
  "name": "Create Primitive",
  "category": "Blender",
  "description": "Create a basic primitive mesh in Blender",
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "type": {"type": "string", "enum": ["cube", "uv_sphere", "ico_sphere", "cylinder"]},
      "size": {"type": "number", "minimum": 0.01}
    },
    "required": ["type"],
    "additionalProperties": true
  }
}

